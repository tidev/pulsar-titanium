{
  "name": "titanium",
  "main": "./lib/index",
  "version": "3.0.6",
  "description": "Titanium build tools and UI for Pulsar",
  "repository": "https://github.com/tidev/pulsar-titanium",
  "license": "Apache-2.0",
  "engines": {
    "atom": ">1.21.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "keywords": [
    "axway",
    "appcelerator",
    "titanium",
    "alloy",
    "mobile",
    "javascript",
    "ios",
    "android"
  ],
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "4.0.0": "provideAutoComplete"
      }
    },
    "hyperclick": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "atomTestRunner": "./spec/test-runner.js",
  "dependencies": {
    "@primer/octicons": "^19.6.0",
    "etch": "^0.14.1",
    "find": "^0.3.0",
    "fs-extra": "^11.1.1",
    "mkdirp": "^3.0.1",
    "semver": "^7.5.4",
    "sudo-prompt": "^9.2.1",
    "titanium-editor-commons": "^2.1.0",
    "underscore": "^1.13.6",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@atom/mocha-test-runner": "^1.6.1",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.47.0",
    "eslint-config-axway": "^8.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "pinst": "^3.0.0",
    "semantic-release": "^21.0.9",
    "semantic-release-pulsar-config": "^1.0.0",
    "sinon": "^15.2.0"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint --ext .js,.jsx .",
    "release": "cross-env HUSKY=0 semantic-release",
    "test": "pulsar --version && pulsar --test spec",
    "postinstall": "husky install || exit 0",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "release": {
    "extends": "semantic-release-pulsar-config",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ],
      "@semantic-release/changelog",
      "semantic-release-pulsar",
      "@semantic-release/git"
    ]
  }
}
